# psend -> precv
#
paws load resources.paws

#set DEBUG 17

set psend [runPawsTask particle-send $HOST 3]
set precv  [runPawsTask particle-recv $HOST 2]

# Connect programs
connectPawsTasks $psend $precv

# Connect datas
# psend:C -> precv:D
#
connectPawsData [ paws get data $psend R  ] [ paws get data $precv R ]
connectPawsData [ paws get data $psend ID ] [ paws get data $precv ID ]
connectPawsData [ paws get data $psend QM ] [ paws get data $precv QM ]

# Then go......
paws go $psend $precv
